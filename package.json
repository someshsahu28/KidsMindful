    "@mui/icons-material": "^7.1.0",
    "@mui/material": "^7.1.0",
    "axios": "^1.9.0",
    "framer-motion": "^12.12.1",
    "react-confetti": "^6.4.0",
    "react-router-dom": "^7.6.0"
  }
}
